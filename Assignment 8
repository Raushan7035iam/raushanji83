Question 8:
#include<stdio.h>
int main()
{
    int i,j;
    for(i=1;i<=4;i++)
    {
        for(j=1;j<=7;j++)
           {
             if(j<=4-i||j>=4+i)
               printf(" ");
             else
               printf("%d",j);
           }
        printf("\n");
    }
    return 0;
}
Question 9:
#include<stdio.h>
int main()
{
    int i,j;
    for(i=1;i<=4;i++)
    {
       for(j=1;j<=4-i;j++)
           printf(" ");
       for(j=1;j<=i;j++)
          printf("%d",j);
       printf("\b");
       for(j=i;j>=1;j--)
          printf("%d",j);
       printf("\n");
    }
    return 0;
}
Question 10:
#include<stdio.h>
int main()
{
    int i,j;
    for(i=4;i>=1;i--)
    {
        for(j=1;j<=4;j++)
        {
            if(j<=i)
                printf("%d",j);
            else
                printf(" ");
        }
        printf("\b");
        for(j=4;j>=1;j--)
        {
            if(j<=i)
                printf("%d",j);
            else
                printf(" ");
        }
        printf("\n");
    }
    return 0;
}
Question 11:
#include<stdio.h>
int main()
{
    int i,j;
    for(i=65;i<=69;i++)
    {
       for(j=69;j>=i;j--)
           printf(" ");
        for(j=65;j<=i;j++)
           printf("%c",j);
           printf("\b");
        for(j=i;j>=65;j--)
           printf("%c",j);
        printf("\n");
    }
}
Question 12:
#include<stdio.h>
int main()
{
    int i,j;
    for(i=68;i>=65;i--)
    {
        for(j=68;j>=i;j--)
            printf(" ");
        for(j=65;j<=i;j++)
            printf("%c",j);
            printf("\b");
        for(j=i;j>=65;j--)
            printf("%c",j);
        printf("\n");
    }
}
Question 13:
#include <stdio.h>

int main() 
{
    int i,j;
    for(i=71;i>=65;i--)
    {
        for(j=65;j<=71;j++)
        {
            if(j<=i)
                printf("%c",j);
            else
                printf(" ");
        }
        printf("\b");
        for(j=71;j>=65;j--)
        {
            if(j<=i)
                printf("%c",j);
            else
                printf(" ");
        }
        printf("\n");
    }

    return 0;
}
Question 14:
#include<stdio.h>
int main()
{
	int i,j;
	for(i=1;i<=5;i++)
	{
		for(j=1;j<=5;j++)
		{
			if(j==1 || j==i)
			printf("*");
			else
			printf(" ");
		}
		printf("\n");
	}
	for(j=1;j<=5;j++)
	{
		printf("*");
	}
	printf("\n");
}
Question 15:
#include<stdio.h>
int main()
{
	int i,j;
	for(i=1;i<=5;i++)
	{
		for(j=1;j<=5;j++)
		{
			if(j==6-i || j==5)
			printf("*");
			else
			printf(" ");
		}
		printf("\n");
}
	for(j=1;j<=5;j++)
	{
		printf("*");
	}
	printf("\n");
}
