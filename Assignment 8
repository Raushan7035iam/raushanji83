		{
			if(j==6-i || j==4+i)
			printf("*");
			else
			printf(" ");
		}
		printf("\n");
	}
	for(j=1;j<=9;j++)
	{
		printf("*");
	}
	printf("\n");
}
Question 17:
#include<stdio.h>
int main()
{
	int i,j;
	for(j=1;j<=9;j++)
	{
		printf("*");
	}
	printf("\n");
	for(i=1;i<=5;i++)
	{
		for(j=1;j<=9;j++)
		{
			if(j==i || j==10-i)
			printf("*");
			else
			printf(" ");
		}
		printf("\n");
	}
}
Question 18:
#include <stdio.h>
int main() 
{
    int i,j;
    for(i=1;i<=9;i++)
    {
        for(j=1;j<=9;j++)
        {
            if(((i<=5)&&(j<=5-i||j>=5+i))||((i>5)&&(j<=i-5||j>=15-i)))
                printf(" ");
            else
                printf("*");            
        } 
        printf("\n");
    }
    return 0;
}
Question 19:
#include<stdio.h>
int main() 
{
    int i,j;
    for(i=1;i<=3;i++)
    {
        for(j=1;j<=10;j++)
        {
            if((j<=3-i)||(j>=7+i))
                 printf(" ");
           else 
                 printf("*");  
        }
        for(j=1;j<=9;j++)
        {
            if(j<=3-i||j>=7+i)
                printf(" ");
            else
                printf("*");
         }
        printf("\n");
    }
            for(i=1;i<=6;i++)
                printf("*");
                printf("MySirG");
            for(i=1;i<=7;i++)
                printf("*");
        printf("\n");
    for(i=1;i<=9;i++)
    {
        for(j=1;j<=19;j++)
            if(j<=i||j>=20-i)
                printf(" ");
            else 
                printf("*");
        printf("\n");
    }
}
